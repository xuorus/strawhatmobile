{
  "expo": {
    "name": "Student Calculator",
    "slug": "strawhatmobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoQuickGPA.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.navareskenneth19.App"
    },
    "web": {
      "favicon": "./assets/images/Logo.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "63e940ac-2d3d-4bd9-a0da-a232b018ac50"
      }
    }
  }
}
